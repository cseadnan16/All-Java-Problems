package pattern_problem;

public class E {

	public static void main(String[] args) {
	int j,i;
		
		for(i=1;i<=7;i++)
		{
			if(i==1 || i==7)
			{
				for(j=0;j<7;j++)
				{
					System.out.print("# ");
				}
				System.out.println();
			}
			else
			{
				for(j=1;j<=7;j++)
				{
					if(j==1 || j==7){
						System.out.print("#    \t");
					}
				}
				System.out.println();
			}
				
		}

	}

}
